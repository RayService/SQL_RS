(  CASE  WHEN (#TabPorPS.mat + #TabPorPS.koop + #TabPorPS.NespecNakl + #TabPorPS.OPN) = 0 THEN -100  WHEN (#TabPorPS.mat + #TabPorPS.koop + #TabPorPS.NespecNakl + #TabPorPS.OPN) <> 0 THEN (((#TabPorPS.mat + #TabPorPS.koop + #TabPorPS.NespecNakl + #TabPorPS.OPN) - (#TabPorPS.mat_Pl + #TabPorPS.koop_Pl + #TabPorPS.OPN_Pl))/(#TabPorPS.mat + #TabPorPS.koop + #TabPorPS.NespecNakl + #TabPorPS.OPN)*100)  ELSE  NULL  END  )
