(  SELECT  (SELECT  ROUND(ISNULL(SUM(tp.TAC_H)+SUM(tp.TBC_H),0),2) AS Time  FROM hvw_APSLogis_OUT_DEMANDORDER dor WITH(NOLOCK)  LEFT OUTER JOIN TabKmenZbozi tkz ON tkz.SkupZbo=LEFT(dor.ITEM_ID,3) AND tkz.RegCis=RIGHT(dor.ITEM_ID, CHARINDEX('|',REVERSE(dor.ITEM_ID))-1)  LEFT OUTER JOIN TabPostup tp WITH(NOLOCK) ON tp.dilec=tkz.ID  LEFT OUTER JOIN TabCzmeny tczOd WITH(NOLOCK) ON tp.ZmenaOd=tczOd.ID  LEFT OUTER JOIN TabCzmeny tczDo WITH(NOLOCK) ON tp.ZmenaDo=tczDo.ID  LEFT OUTER JOIN TabCPraco tcpo WITH(NOLOCK) ON tp.pracoviste=tcpo.ID  LEFT OUTER JOIN TabCPraco_EXT tcpoe WITH(NOLOCK) ON tcpoe.ID=tcpo.ID  WHERE  (((tczOd.platnostTPV=1 AND tczOd.datum<=GETDATE()))AND(((tczDo.platnostTPV=0 OR tp.ZmenaDo IS NULL OR (tczDo.platnostTPV=1 AND tczDo.datum>GETDATE())) )))  AND((ISNULL(tcpoe._EXT_RS_wokplaceQA,0)=0)AND(ISNULL(tcpoe._EXT_RS_workplaceWH,0)=0)AND(ISNULL(tcpoe._EXT_RS_workplaceTPP,0)=0)AND(ISNULL(tcpoe._EXT_RS_workplaceTEW,0)=0))  AND tp.typ=1  AND dor.ID=dord.ID)*dord.QTY_OPEN  FROM hvw_APSLogis_OUT_DEMANDORDER dord WITH(NOLOCK)  WHERE dord.ID=hvw_APSLogis_OUT_DEMANDORDER.ID  )
