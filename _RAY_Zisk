((SELECT TabPohybyZbozi.CCbezDaniKcPoS FROM #TabPorPS PPS   INNER JOIN TabPrikaz WITH(NOLOCK) ON Tabprikaz.ID = PPS.IDPrikaz  INNER JOIN TabPohybyZbozi WITH(NOLOCK) ON  TabPohybyZbozi.ID = TabPrikaz. IDRezervace  WHERE PPS.ID = #TabPorPS.ID ) - (SELECT (mzda*6.37)+(mat*1.15)+(koop*1.15) FROM #TabPorPS PPS WHERE PPS.ID = #TabPorPS.ID))  
