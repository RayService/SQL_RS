(  CASE  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 95 AND 105 THEN 1  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 90 AND 94.999 THEN 2  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 105.0001 AND 110 THEN 2  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 50 AND 89.999 THEN 3  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 110.001 AND 150 THEN 3  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 < 50 THEN 4  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 > 150 THEN 4  WHEN (SELECT Celkem FROM #TabPorPS PPS WHERE PPS.ID= #TabPorPS.ID) = 0 THEN NULL  ELSE  NULL  END  )
