CAST(CASE WHEN ((SELECT _material FROM TabPrikaz_EXT WHERE ID=TabPrikaz.ID)+10) > (TabPrikaz.Plan_zadani)   AND  (TabPrikaz.StavPrikazu = 20)   THEN 6881412    WHEN (TabPrikaz.Plan_zadani) > (GetDate() +21)   AND  (TabPrikaz.StavPrikazu = 30)   AND  (SELECT U.datum_X FROM TabPlan U WITH(NOLOCK) WHERE U.ID=TabPrikaz.IDPlan) < (SELECT U.Plan_ukonceni_X FROM TabPrikaz U WHERE U.ID=TabPrikaz.ID)  THEN 425216      END AS INT)  
