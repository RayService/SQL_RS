(  SELECT TOP (1) DATEPART(YEAR,convert(nvarchar(255),TabZmenovyLOG.Datum, 20))  FROM TabZmenovyLOG WITH(NOLOCK)  WHERE  ((((TabZmenovyLOG.Tabulka=N'TabPrikaz')   AND TabZmenovyLOG.IDvTab=TabPrikaz.ID)))AND(TabZmenovyLOG.LogAkce LIKE N'u%')AND(TabZmenovyLOG.Tabulka LIKE N'TabPrikaz%')AND(TabZmenovyLOG.Sloupec LIKE N'plan_ukonceni%') AND convert(nvarchar(255),TabZmenovyLOG.New, 20) > convert(nvarchar(255),GETDATE(), 20)   AND TabZmenovyLOG.IdZurnal =          (SELECT MAX(IdZurnal) FROM TabZmenovyLOG WITH(NOLOCK)         WHERE         ((((TabZmenovyLOG.Tabulka=N'TabPrikaz')          AND TabZmenovyLOG.IDvTab=TabPrikaz.ID)))AND(TabZmenovyLOG.LogAkce LIKE N'u%')AND(TabZmenovyLOG.Tabulka LIKE N'TabPrikaz%')AND(TabZmenovyLOG.Sloupec LIKE N'plan_ukonceni%') AND convert(nvarchar(255),TabZmenovyLOG.New, 20) > convert(nvarchar(255),GETDATE(), 20))  )
