(SELECT    ISNULL(    MIN(VE._KontrolaPokryti_Vysledek)    ,(SELECT MIN(VVE._KontrolaPokryti_Vysledek)     FROM TabPlanPrKVazby VV WITH(NOLOCK)     INNER JOIN TabPlanPrKVazby_EXT VVE WITH(NOLOCK) ON VV.ID = VVE.ID    WHERE VV.IDPlanPrikaz = TabPlanPrikaz.ID)    )  FROM TabPlanPrKVazby V WITH(NOLOCK)  INNER JOIN TabPlanPrKVazby_EXT VE WITH(NOLOCK) ON V.ID = VE.ID  WHERE V.IDPlanPrikaz = TabPlanPrikaz.ID   AND VE._KontrolaPokryti_Vysledek > 0)
