(SELECT (PZ.Mnozstvi - ((SELECT Mnozstvi FROM TabPohybyZbozi P WITH(NOLOCK)     JOIN TabDokladyZbozi D ON D.ID = P.IDDoklad        WHERE D.DruhPohybuZbo = 11 AND PZ.ID = P.ID)  -  (SELECT ISNULL(SUM(P2.Mnozstvi),0) FROM TabPohybyZbozi P2 WITH(NOLOCK)     JOIN TabPohybyZbozi P1 WITH(NOLOCK) ON P1.ID = P2.IDOldPolozka     WHERE PZ.ID = P1.ID))   ) * PZ.JCbezDaniKC FROM TabPohybyZbozi PZ WITH(NOLOCK)  WHERE PZ.DruhPohybuZbo = 9 AND  PZ.ID = TabPohybyZbozi.ID)
