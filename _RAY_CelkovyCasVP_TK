(  SELECT (SUM((PP.Kusy_zad * PP.TAC_H)+PP.TBC_H+PP.TEC_H))  FROM TabPrPostup PP WITH(NOLOCK)  LEFT JOIN TabCPraco WITH(NOLOCK) ON TabCPraco.ID=PP.Pracoviste  LEFT OUTER JOIN TabCPraco_EXT  WITH(NOLOCK) ON TabCPraco_EXT.ID=TabCPraco.ID  WHERE ((TabCPraco.IDTabStrom LIKE N'2000026%' OR (TabCPraco.IDTabStrom LIKE N'2002%'AND ISNULL(TabCPraco_EXT._EXT_RS_wokplaceQA,0)=1))) AND PP.IDPrikaz=TabPrikaz.ID AND PP.IDOdchylkyDo IS NULL AND PP.prednastaveno=1  )
