(CASE WHEN EXISTS(SELECT koo.ID  FROM TabPolKoopObj koopol WITH (NOLOCK)  LEFT OUTER JOIN TabKmenZbozi tkz WITH (NOLOCK) ON tkz.ID=(SELECT PrP.Dilec FROM TabPrPostup PrP WITH (NOLOCK) WHERE PrP.IDPrikaz=koopol.IDPrikaz AND PrP.Doklad=koopol.DokladPrPostup AND PrP.Alt=koopol.AltPrPostup AND PrP.IDOdchylkyDo IS NULL)  LEFT OUTER JOIN TabKoopObj koo WITH (NOLOCK) ON koo.ID=koopol.IDObjednavky  WHERE (koopol.IDObjednavky=TabKoopObj.ID) AND ((tkz.SkupZbo=N'800')OR(tkz.SkupZbo=N'850')))  THEN 16711680 ELSE NULL END)
