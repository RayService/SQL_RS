(  SELECT TOP 1 nce._RAY_Part_number  FROM TabKmenZbozi tkz WITH(NOLOCK)  LEFT OUTER JOIN TabNc nc WITH(NOLOCK) ON nc.IDKmenZbozi = tkz.ID  LEFT OUTER JOIN TabNC_EXT nce WITH(NOLOCK) ON nce.ID = nc.ID  WHERE nce._RAY_Part_number IS NOT NULL AND nc.CenaVal1 = (SELECT MIN(nc.CenaVal1) FROM TabNC nc WITH(NOLOCK) WHERE nc.IDKmenZbozi = tkz.ID AND nc.CenaVal1 > 0) AND tkz.ID = TabKmenZbozi.ID  )
