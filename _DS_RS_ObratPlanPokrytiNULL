(  SELECT SUM(ISNULL(tss.Prumer,0)*ISNULL(PPrKV.mnoz_zad,0))        FROM TabPlanPrKVazby PPrKV WITH(NOLOCK)        LEFT OUTER JOIN TabPlanPrKVazby_EXT tplprke WITH(NOLOCK) ON tplprke.ID=PPrKV.ID     LEFT OUTER JOIN TabStavSkladu tss WITH(NOLOCK) ON tss.IDKmenZbozi=PPrKV.nizsi AND tss.IDSklad=PPrKV.Sklad  WHERE PPrKV.IDPlan=hvw_86DA7EFB9A1C47909A80C388875A2530.IDPolozka AND hvw_86DA7EFB9A1C47909A80C388875A2530.Typ=0 AND tplprke._KontrolaPokryti_Vysledek IS NULL  GROUP BY PPrKV.IDPlan  )
