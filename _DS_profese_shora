(SELECT TOP 1 x.Profese FROM TabVyrProfese x WITH(NOLOCK)   INNER JOIN TabPrPostupVyrProfese WITH(NOLOCK) ON x.ID = TabPrPostupVyrProfese.IDVyrProfese  INNER JOIN TabPrPostup WITH(NOLOCK) ON TabPrPostupVyrProfese.IDPostup = TabPrPostup.ID  INNER JOIN TabPrikaz WITH(NOLOCK) ON TabPrPostup.IDPrikaz = TabPrikaz.ID  INNER JOIN TabPrikazMzdyAZmetky AS y WITH(NOLOCK) ON TabPrikaz.ID = TabPrikazMzdyAZmetky.IDPrikaz  WHERE y.ID= TabPrikazMzdyAZmetky.ID)
