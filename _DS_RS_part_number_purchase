(  SELECT NCE._RAY_Part_number  FROM TabPohybyZbozi tpz WITH(NOLOCK)  LEFT OUTER JOIN TabDokladyZbozi tdz WITH(NOLOCK) ON tdz.ID=tpz.IDDoklad  LEFT OUTER JOIN TabCisOrg tco WITH(NOLOCK) ON tco.CisloOrg=tdz.CisloOrg  LEFT OUTER JOIN TabStavSkladu tss WITH(NOLOCK) ON tss.ID=tpz.IDZboSklad  LEFT OUTER JOIN TabNC nc WITH(NOLOCK) ON nc.CenovaUroven=tco.CenovaUrovenNakup AND nc.IDKmenZbozi=tss.IDKmenZbozi  LEFT OUTER JOIN TabNC_EXT nce WITH(NOLOCK) ON nce.ID=nc.ID  WHERE tpz.ID=TabPohybyZbozi.ID  )
