(  SELECT CASE ROUND(sdpol.mnozstvi,3) WHEN 0.0 THEN 0.0 ELSE sddoc.DobaVyskladneni/(SELECT COUNT(sdpol.ID)  FROM hvw_TermNasnimPol sdpol WITH(NOLOCK)  LEFT OUTER JOIN hvw_Gatema_SDDoklady sddoc WITH(NOLOCK) ON sddoc.ID=sdpol.IDDokladSD  WHERE sddoc.ID = hvw_TermNasnimPol.IDDokladSD AND sdpol.mnozstvi<>0) END  FROM hvw_TermNasnimPol sdpol WITH(NOLOCK)  LEFT OUTER JOIN hvw_Gatema_SDDoklady sddoc WITH(NOLOCK) ON sddoc.ID=sdpol.IDDokladSD  WHERE sdpol.ID = hvw_TermNasnimPol.ID  )
