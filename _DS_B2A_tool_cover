(  SELECT ROUND(   (SELECT     CAST(COUNT(DISTINCT PNV.ID) as float)    FROM TabPrNVazby PNV WITH(NOLOCK)    LEFT JOIN TabKmenZbozi Z WITH(NOLOCK) on PNV.Naradi = Z.ID     LEFT JOIN B2A_TermETH_ToolUsage_EXT E WITH(NOLOCK) on PNV.ID = E.IDTool    WHERE     PNV.IDOdchylkyDo IS NULL AND     Z.Naradi = 1 AND     Z.Blokovano = 0 AND     E.ID IS NOT NULL AND      (E.ToolNewLink IS NULL OR E.ToolNewLink = 0) AND     PNV.IDPrikaz = TabPrikaz.ID   )  /NULLIF(     (      (SELECT CAST(COUNT(PNV.ID) as float)       FROM TabPrNVazby PNV WITH(NOLOCK)        LEFT JOIN TabKmenZbozi Z WITH(NOLOCK) on PNV.Naradi = Z.ID        LEFT JOIN B2A_TermETH_ToolUsage_EXT E WITH(NOLOCK) on PNV.ID = E.IDTool       WHERE        PNV.IDOdchylkyDo IS NULL AND        Z.Naradi = 1 AND        Z.Blokovano = 0 AND        PNV.IDPrikaz = TabPrikaz.ID AND        (E.ToolNewLink IS NULL OR E.ToolNewLink = 0)      ) / 100.0     )     , 0   )  ,0))
