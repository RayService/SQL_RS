(  SELECT ISNULL(  (SELECT TOP 1 ISNULL(jag.Potvrzeny_TDOD,0)  FROM hvw_APSLogis_OUT_PURCHASEMODPROP prop WITH(NOLOCK)  LEFT OUTER JOIN Tabx_RS_OUT_MANUFACTURINGORDERJAGGING jag WITH(NOLOCK) ON prop.PURCHASE_ORDER_ID=jag.ID_Dodavky AND jag.Typ_Dodavky='PO'  WHERE prop.ID=hvw_APSLogis_OUT_PURCHASEMODPROP.ID AND jag.ID_Dodavky IS NOT NULL AND jag.Typ_Dodavky='PO'  ORDER BY jag.Potvrzeny_TDOD DESC),0)  )
