(  CAST((  CASE WHEN EXISTS (SELECT *  FROM (SELECT tprp.ID, tprp.pracoviste AS workplace,    LEAD(tprp.pracoviste) OVER (ORDER BY tprp.Operace) AS next_workplace      FROM TabPrPostup tprp WITH(NOLOCK)   WHERE tprp.IDPrikaz=(SELECT tpp.IDPrikaz FROM TabPrPostup tpp WITH(NOLOCK) WHERE tpp.ID=TabPrPostup.ID) AND tprp.IDOdchylkyDo IS NULL  ) AS T  WHERE T.ID=TabPrPostup.ID AND T.workplace<>T.next_workplace)  THEN 1 ELSE 0 END) AS BIT)  )
