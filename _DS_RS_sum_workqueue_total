(  SELECT  ISNULL(SUM((tpp.Kusy_zad * tpp.TAC_S / (3600.0 * tpp.DavkaTPV_VO))+tpp.TBC_H),0)  FROM TabPrPostup tpp WITH(NOLOCK)  LEFT OUTER JOIN TabPrikaz tp WITH(NOLOCK) ON tpp.IDPrikaz=tp.ID  LEFT OUTER JOIN TabCOdchylek tco WITH(NOLOCK) ON tpp.IDOdchylkyOd=tco.ID  WHERE  ((tp.StavPrikazu IN (20,30) AND tpp.splneno=0 AND tpp.IDOdchylkyDo IS NULL)  AND((tpp.pracoviste=TabCPraco.ID))  )    )
