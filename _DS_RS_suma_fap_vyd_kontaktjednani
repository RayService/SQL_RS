(  SELECT  SUM(TabDokladyZbozi.SumaKcBezDPH)  FROM TabDokladyZbozi WITH(NOLOCK)  WHERE  ((((TabDokladyZbozi.DruhPohybuZbo>=2)AND(TabDokladyZbozi.DruhPohybuZbo<=4))OR((TabDokladyZbozi.DruhPohybuZbo>=18)AND(TabDokladyZbozi.DruhPohybuZbo<=19)))AND(TabDokladyZbozi.PoradoveCislo>=0 AND EXISTS(SELECT * FROM TabVKonJedDokZbo V WITH(NOLOCK) WHERE V.IDDokZbo=TabDokladyZbozi.ID AND V.IDKonJed=TabKontaktJednani.ID)) AND (TabDokladyZbozi.Obdobi=:__IDOBDOBI))  )
