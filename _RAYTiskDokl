(CAST (  CASE  WHEN   (SELECT SUM(CAST(TabPohybyZbozi_EXT._Tisk as INT))  FROM TabDokladyZbozi as DZ WITH(NOLOCK)  JOIN TabPohybyZbozi WITH(NOLOCK) ON DZ.ID=TabPohybyZbozi.IDDoklad  JOIN TabPohybyZbozi_EXT WITH(NOLOCK) ON TabPohybyZbozi_EXT.ID=TabPohybyZbozi.ID  WHERE DZ.ID=TabDokladyZbozi.ID) = (SELECT count(*) FROM TabPohybyZbozi WITH(NOLOCK) WHERE TabPohybyZbozi.IdDoklad = TabDokladyZbozi.ID)   THEN 1  ELSE 0  END  AS BIT)  )
