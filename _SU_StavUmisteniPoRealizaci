(SELECT ISNULL(SUM(SU.Mnozstvi),0.0) FROM Gatema_StavUmisteni SU WITH(NOLOCK) WHERE SU.IDStavSkladu=TabStavSkladu.ID) - (SELECT ISNULL(SUM(PU.Mnozstvi),0.0) FROM Gatema_PohybUmisteni PU WITH(NOLOCK) LEFT OUTER JOIN TabPohybyZbozi PZ WITH(NOLOCK) ON (PZ.ID=PU.IDPohZbo AND PZ.SkutecneDatReal IS NOT NULL) WHERE PU.IDStavSkladu=TabStavSkladu.ID AND PU.DruhPohybu IN (1,2) AND PZ.ID IS NULL)
