CAST(STUFF(   (SELECT ',' + D.ParovaciZnak   FROM TabPohybyZbozi P WITH(NOLOCK)    INNER JOIN TabDokladyZbozi D WITH(NOLOCK) ON P.IDDoklad = D.ID   WHERE P.IDPrikaz = TabPrKVazby.IDPrikaz     AND P.DokladPrikazu = TabPrKVazby.Doklad    AND P.TypVyrobnihoDokladu = 1    AND P.SkutecneDatReal IS NOT NULL    AND (SELECT SS.IDKmenZbozi FROM TabStavSkladu SS WITH(NOLOCK) WHERE SS.ID = P.IDZboSklad) = TabPrKVazby.nizsi   FOR XML PATH(''))   ,1,1,'') as NVARCHAR(255))
