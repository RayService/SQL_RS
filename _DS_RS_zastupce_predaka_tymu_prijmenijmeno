(  SELECT  tcz1.prijmenijmeno  FROM TabCisZam tcz WITH(NOLOCK)    LEFT OUTER JOIN TabCisZam_EXT tcze WITH(NOLOCK) ON tcze.ID=tcz.ID    LEFT OUTER JOIN TabStrom ts WITH(NOLOCK) ON ts.ID = tcze._EXT_B2ADIMA_ProductionUnitId    LEFT OUTER JOIN TabStrom_EXT tse WITH(NOLOCK) ON tse.ID = ts.ID    LEFT OUTER JOIN TabCisZam tcz1 WITH(NOLOCK) ON tcz1.ID = tse._EXT_B2ADIMA_ForemanAlternateId  WHERE tcz.ID = TabCisZam.ID  )
