( SELECT KZE._material * P.kusy_zad  FROM #TabPorPS PPS  LEFT OUTER JOIN TabPrikaz P WITH(NOLOCK) ON #TabPorPS.IDPrikaz=P.ID  LEFT OUTER JOIN TabKmenZbozi KZ  WITH(NOLOCK) ON PPS.IDKmenZbozi= KZ.ID    LEFT OUTER JOIN TabKmenZbozi_EXT KZE  WITH(NOLOCK) ON KZ.ID= KZE.ID    WHERE PPS.Id = #TabPorPS.Id)
