(SELECT TOP 1 SDU.Jmeno FROM Gatema_PohybUmisteni_EXT PU_E WITH(NOLOCK)  INNER JOIN Gatema_PohybUmisteni PU WITH(NOLOCK)  ON PU.ID=PU_E.ID   LEFT OUTER JOIN SDServer_Users SDU WITH(NOLOCK)  ON SDU.Login = PU_E.Autor  WHERE PU.IDStavSkladu=hvw_RozdilKVydeji.IDStavSkladu AND   ISNULL(PU.IDVyrCis, 0)=ISNULL(hvw_RozdilKVydeji.IDVyrCis, 0) AND   PU.IDUmisteni=hvw_RozdilKVydeji.IDUmisteni ORDER BY PU.ID DESC)
