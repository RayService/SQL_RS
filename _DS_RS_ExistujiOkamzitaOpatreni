(  CAST((CASE WHEN EXISTS (SELECT  TabUkoly.ID  FROM TabUkoly WITH(NOLOCK)  LEFT OUTER JOIN TabKontaktJednani tkj WITH(NOLOCK) ON TabUkoly.IDKontaktJed=tkj.ID AND TabUkoly.Vzor=0  WHERE  ((TabUkoly.Vzor=0)AND(EXISTS(SELECT*FROM TabKategKontJed K WITH(NOLOCK) JOIN TabKontaktJednani KJ ON KJ.Kategorie=K.Cislo WHERE K.QMSAgenda=2 AND TabUkoly.IDKontaktJed=KJ.ID))AND(TabUkoly.IDKontaktJed=TabKontaktJednani.ID)))   THEN 1 ELSE 0 END)  AS BIT)  )
