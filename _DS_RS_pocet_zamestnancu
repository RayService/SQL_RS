(  SELECT COUNT(tcz.ID)  FROM TabCisZam tcz WITH(NOLOCK)  LEFT OUTER JOIN TabPersVyberRizeniZamestnanec pvrz WITH(NOLOCK) ON pvrz.ZamestnanecID = tcz.ID  JOIN TabPersVyberRizeni pvr WITH(NOLOCK) ON pvrz.VyberRizeniId = pvr.ID  WHERE pvr.ID = TabPersVyberRizeni.ID  AND((ISNULL((SELECT TabZamMzd.StavES FROM TabZamMzd WITH(NOLOCK) WHERE TabZamMzd.IdObdobi=(SELECT  tabmzdobd.ID  FROM TabMzdObd WITH(NOLOCK)  WHERE TabMzdObd.Mes = (SELECT DATEPART(MONTH,GETDATE())) AND TabMzdObd.Rok = (SELECT DATEPART(YEAR,GETDATE()))) AND TabZamMzd.ZamestnanecId=tcz.ID), 10)))=0  )
