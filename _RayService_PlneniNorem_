(  CASE WHEN (SELECT ISNULL(SUM(MZ.Sk_cas_H),0) FROM TabPrikazMzdyAZmetky MZ WITH(NOLOCK) WHERE MZ.IDPrikaz=TabPrikaz.ID AND MZ.stav=1)=0 THEN 0  ELSE  ((SELECT SUM((tpp.kusy_zad*tpp.TAC_H)+tpp.TBC_H+tpp.TEC_H)  FROM TabPrPostup tpp  WHERE tpp.IDPrikaz=TabPrikaz.ID AND tpp.IDOdchylkyDo IS NULL)  /  (SELECT SUM(MZ.Sk_cas_H) FROM TabPrikazMzdyAZmetky MZ WITH(NOLOCK) WHERE MZ.IDPrikaz=TabPrikaz.ID AND MZ.stav=1)  *100)  END  )
