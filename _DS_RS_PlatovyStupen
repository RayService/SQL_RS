(  SELECT TOP 1 mzd.PlatovyStupen  FROM hvw_03B1C3CBC4514F48B3E02D1DFCA91364 schit WITH(NOLOCK)  LEFT OUTER JOIN TabZamMzd_EXT mzde WITH(NOLOCK) ON mzde._EXT_RS_IDRatingSchema=schit.IDSchema  LEFT OUTER JOIN TabZamMzd mzd WITH(NOLOCK) ON mzd.ID=mzde.ID  WHERE schit.ID=hvw_03B1C3CBC4514F48B3E02D1DFCA91364.ID  ORDER BY mzd.ID DESC  )    
