(  SELECT CASE   WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 1 THEN 'Jan'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 2 THEN 'Feb'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 3 THEN 'Mar'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 4 THEN 'Apr'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 5 THEN 'May'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 6 THEN 'Jun'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 7 THEN 'Jul'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 8 THEN 'Aug'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 9 THEN 'Sep'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 10 THEN 'Oct'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 11 THEN 'Nov'  WHEN (SELECT DATEPART(MONTH,TabDokladyZbozi.DatPorizeni)) = 12 THEN 'Dec'  ELSE NULL  END  )
