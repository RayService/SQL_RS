(  SELECT CASE WHEN tcz.ID IN (SELECT TabZamMzd.ZamestnanecId  FROM TabZamMzd WITH(NOLOCK)  WHERE  (TabZamMzd.IdObdobi=(SELECT tabmzdobd.ID FROM TabMzdObd WITH(NOLOCK) WHERE TabMzdObd.Mes = (SELECT DATEPART(MONTH,GETDATE())) AND TabMzdObd.Rok = (SELECT DATEPART(YEAR,GETDATE()))))  AND((TabZamMzd.DuvodUkonceniPP=N'02')OR(TabZamMzd.DuvodUkonceniPP=N'13'))) THEN 1 ELSE 0 END AS BIT  FROM TabCisZam tcz WITH(NOLOCK)  WHERE tcz.ID = TabCisZam.ID  )
