(  SELECT TOP 1 pau.Koruny  FROM hvw_03B1C3CBC4514F48B3E02D1DFCA91364 schit WITH(NOLOCK)  LEFT OUTER JOIN TabZamMzd_EXT mzde WITH(NOLOCK) ON mzde._EXT_RS_IDRatingSchema=schit.IDSchema  LEFT OUTER JOIN TabZamMzd mzd WITH(NOLOCK) ON mzd.ID=mzde.ID-- AND mzd.IdObdobi=277  LEFT OUTER JOIN TabMzPaus pau WITH(NOLOCK) ON mzd.ZamestnanecId=pau.ZamestnanecId AND pau.IdObdobi=mzd.IdObdobi AND pau.CisloMS=922  WHERE schit.ID=hvw_03B1C3CBC4514F48B3E02D1DFCA91364.ID AND mzd.PlatovaTrida<>'SP'  ORDER BY mzd.ID DESC  )    
