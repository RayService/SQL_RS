CAST (CASE WHEN EXISTS(SELECT *    FROM TabPohybyZbozi PZ  WITH(NOLOCK)     INNER JOIN TabStavSkladu SS WITH(NOLOCK) ON SS.ID=PZ.IDZboSklad      INNER JOIN TabKmenZbozi KZ WITH(NOLOCK) ON KZ.ID=SS.IDKmenZbozi     OUTER APPLY (SELECT Mnozstvi=ISNULL(SUM(PU.Mnozstvi),0.0) FROM hvw_Gatema_PohybUmisteniAll PU WITH(NOLOCK) WHERE PU.IDPohZbo=PZ.ID) Um    WHERE PZ.IDDoklad=TabDokladyZbozi.ID AND ABS(PZ.MnozstviDruhove*PZ.PrepMnozstvi - Um.Mnozstvi)>=0.0001 AND KZ.Sluzba=0) THEN 0 ELSE 1 END AS BIT)
