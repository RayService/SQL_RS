(SELECT    CASE     WHEN TabPohybyZbozi.PotvrzDatDod IS NOT NULL AND DTBL.RadaDokladu IN (N'601',N'602',N'616',N'662',N'664',N'656',N'655',N'674')     THEN CASE       WHEN DATEDIFF(DAY,DTBL.DatPorizeni,TabPohybyZbozi.PotvrzDatDod) < -5 AND NOT ISNULL(DTBL.DuvodOpozdeni,N'') = N'8. Jiné důvody zákazníka'        THEN 1       ELSE 0      END   END  FROM    (SELECT D.RadaDokladu, D.DatPorizeni, DuvodOpozdeni = CASE WHEN TabPohybyZbozi.DruhPohybuZbo IN (2,4) THEN PEP._LCS_PFA_DuvodOpozdeniExp ELSE PE._LCS_PFA_DuvodOpozdeniExp END   FROM TabDokladyZbozi D WITH(NOLOCK)    INNER JOIN TabPohybyZbozi_EXT PE WITH(NOLOCK) ON PE.ID = TabPohybyZbozi.ID    LEFT OUTER JOIN TabPohybyZbozi_EXT PEP WITH(NOLOCK) ON TabPohybyZbozi.IDOldPolozka = PEP.ID   WHERE D.ID = TabPohybyZbozi.IDDoklad) as DTBL)
