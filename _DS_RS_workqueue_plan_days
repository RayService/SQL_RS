(  (SELECT  ISNULL(SUM((planpp.Kusy_zad * planpp.TAC_S / (3600.0 * planpp.DavkaTPV_VO))+planpp.TBC_H),0)  FROM TabPlanPrPostup planpp WITH(NOLOCK)  LEFT OUTER JOIN TabPlanPrikaz planp WITH(NOLOCK) ON planpp.IDPlanPrikaz=planp.ID  WHERE planpp.pracoviste=TabCPraco.ID)  /  ((convert(Numeric(19,6),(TabCPraco.smennost * 0.01 * TabCPraco.plneni * (SELECT CASE WHEN COUNT(*)=0 THEN 1 ELSE COUNT(*) END FROM TabCisStroju CS WITH(NOLOCK) WHERE CS.IDPrac=TabCPraco.ID) * TabCPraco.vice_lidi))))  )
