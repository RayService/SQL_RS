CAST(         (         SELECT          CASE   WHEN (SELECT _PripFAI FROM TabPrikaz_EXT WITH(NOLOCK) WHERE ID=TabPrikazMzdyAZmetky.IDPrikaz)=1          THEN 16711935         WHEN U.DatumUkonceniOp IS NOT NULL          THEN 11140822         WHEN U.datum_X BETWEEN GetDate()-1 AND GetDate()                  THEN 10477311         WHEN U.datum_X BETWEEN GetDate() AND GetDate()+1                     THEN 9830397         WHEN U.datum_X BETWEEN GetDate()+1 AND GetDate()+7                   THEN NULL         WHEN U.datum_X < GetDATE()                   THEN 9671679         ELSE 11842740         END          FROM TabPrikazMzdyAZmetky U WITH(NOLOCK) WHERE U.ID=TabPrikazMzdyAZmetky.ID         )  AS INT)  
