(  SELECT COUNT(tczemistr.ID)  FROM TabCPraco tcpo WITH(NOLOCK)  LEFT OUTER JOIN TabCisZam_EXT tcze WITH(NOLOCK) ON tcze._EXT_RS_workplace_IDWorkplace=tcpo.ID  JOIN TabCisZam tcz WITH(NOLOCK) ON tcz.ID=tcze.ID  LEFT OUTER JOIN TabCisZam_EXT tczemistr WITH(NOLOCK) ON tczemistr._EXT_RS_workplace_IDForeman=tcz.ID  WHERE tcpo.ID=TabCPraco.ID  )
