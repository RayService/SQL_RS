(  CAST(CASE WHEN (SELECT COUNT(TP.ID)  FROM TabPlan TP WITH(NOLOCK)  INNER JOIN TabPlanZdrojOZ TPZ WITH(NOLOCK) ON TPZ.IDPlan = TP.ID  JOIN TabPohybyZbozi PZ WITH(NOLOCK) ON TPZ.IDRezervace = PZ.ID  JOIN TabDokladyZbozi TD WITH(NOLOCK) ON TD.ID = PZ.IDDoklad  WHERE TD.DruhPohybuZbo = 11 AND TD.RadaDokladu IN (320,330,374) AND TP.Rada IN ('Plan_fix','Plan_quick','P_RO','P_RO_povrz','P_blokace') AND TP.ID = TabPlan.ID) >= 1  THEN 9  ELSE NULL  END AS INT)  )
