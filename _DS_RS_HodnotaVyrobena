(  SELECT  ISNULL(SUM(tpz.CCevidSN),0)  FROM TabPohybyZbozi tpz WITH(NOLOCK)    LEFT OUTER JOIN TabDokladyZbozi VDokladyZboziPohybyZbozi WITH(NOLOCK) ON VDokladyZboziPohybyZbozi.ID=tpz.IDDoklad    LEFT OUTER JOIN TabStavSkladu VPohybyStavSkladu WITH(NOLOCK) ON tpz.IDZboSklad=VPohybyStavSkladu.ID    LEFT OUTER JOIN TabKmenZbozi VPohybyZboKmen WITH(NOLOCK) ON VPohybyZboKmen.ID=(SELECT TabStavSkladu.IDKmenZbozi FROM TabStavSkladu WHERE TabStavSkladu.ID=tpz.IDZboSklad)    LEFT OUTER JOIN TabPohybyZbKalkRoz VPohybyZboziKalkRoz WITH(NOLOCK) ON VPohybyZboziKalkRoz.IDPohyb=tpz.ID  WHERE  (tpz.IDPrikaz=TabPrikaz.ID)AND(tpz.TypVyrobnihoDokladu=0)  )
