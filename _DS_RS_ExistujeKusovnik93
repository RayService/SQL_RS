(  SELECT CAST(CASE WHEN EXISTS(  SELECT tkv.*  FROM TabKvazby tkv WITH(NOLOCK)  LEFT OUTER JOIN TabCzmeny tczOd WITH(NOLOCK) ON tkv.ZmenaOd=tczOd.ID  LEFT OUTER JOIN TabCzmeny tczDo WITH(NOLOCK) ON tkv.ZmenaDo=tczDo.ID  --LEFT OUTER JOIN TabKmenZbozi tkzV WITH(NOLOCK) ON tkzV.ID=tkv.vyssi  LEFT OUTER JOIN TabKmenZbozi tkzN WITH(NOLOCK) ON tkv.nizsi=tkzN.ID  WHERE  (tczOd.platnostTPV=1 AND tczOd.datum<=GETDATE() AND (tczDo.platnostTPV=0 OR tkv.ZmenaDo IS NULL OR (tczDo.platnostTPV=1 AND tczDo.datum>GETDATE())))  --AND tkzV.ID=TabKmenZbozi.ID  AND tkv.vyssi=TabKmenZbozi.ID  AND(tkv.ID IS NOT NULL)  AND(tkzN.SkupZbo LIKE '93%')  ) THEN 0 ELSE 1 END AS BIT)  )
