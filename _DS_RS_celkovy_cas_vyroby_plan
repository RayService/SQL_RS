  (  SELECT  SUM(tppp.Kusy_zad * tppp.TAC_S / 3600.0)+SUM(tppp.TBC_S / 3600.0)  FROM TabPlan tp WITH(NOLOCK)    LEFT OUTER JOIN TabPlanPrikaz tpp WITH(NOLOCK) ON tpp.IDPlan = tp.ID    LEFT OUTER JOIN TabPlanPrPostup tppp WITH(NOLOCK) ON tppp.IDPlanPrikaz = tpp.ID    LEFT OUTER JOIN TabCPraco tcp WITH(NOLOCK) ON tppp.Pracoviste=tcp.ID    LEFT OUTER JOIN TabCPraco_EXT tcpe WITH(NOLOCK) ON tcpe.ID=tcp.ID  WHERE (tcp.IDTabStrom = '200' OR (tcp.IDTabStrom LIKE '2002%' AND (ISNULL(tcpe._EXT_RS_wokplaceQA,0)=0   AND ISNULL(tcpe._EXT_RS_workplaceWH,0)=0  ))) AND tp.ID = TabPlan.ID  )
