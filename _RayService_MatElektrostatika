CAST(CASE WHEN EXISTS(SELECT * FROM TabStavSkladu S WITH(NOLOCK)   INNER JOIN TabKmenZbozi K WITH(NOLOCK) ON S.IDKmenZbozi = K.ID   INNER JOIN TabKvazby KS WITH(NOLOCK) ON K.IdKusovnik = KS.vyssi   LEFT OUTER JOIN TabCzmeny VZmenaOd WITH(NOLOCK) ON KS.ZmenaOd = VZmenaOd.ID   LEFT OUTER JOIN TabCzmeny VZmenaDo WITH(NOLOCK) ON KS.ZmenaDo = VZmenaDo.ID   INNER JOIN TabKmenZbozi_EXT KE ON KS.nizsi = KE.ID  WHERE S.ID = TabPohybyZbozi.IDZboSklad   AND (KS.IDVarianta IS NULL OR KS.IDVarianta = K.IdVarianta)    AND (VZmenaOd.platnostTPV=1 AND VZmenaOd.datum <= GETDATE())   AND (VZmenaDo.platnostTPV=0      OR KS.ZmenaDo IS NULL      OR (VZmenaDo.platnostTPV=1 AND VZmenaDo.datum > GETDATE()))   AND KE._elektrostatika = 1) THEN 1 ELSE 0 END as BIT)
