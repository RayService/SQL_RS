((select (case when sum(1)=0 then 0  else sum(isnull(tsse._rnprumstsklsn,0)) /sum(1) end) from tabstavskladu tss WITH(NOLOCK) left outer join tabstavskladu_ext tsse WITH(NOLOCK) on tsse.id=tss.id   left outer join TabStrom mm WITH(NOLOCK) on mm.cislo=tss.idsklad left outer  join TabStrom_ext mme on mme.id=mm.id  where tss.idsklad=TabStrom.cislo  and isnull(tsse._rnprumstsklsn,0) <>0 and mme._sapvyloucitznobratk is null))
