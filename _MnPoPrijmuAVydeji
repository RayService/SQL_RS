(  TabVyrCS.Mnozstvi+   (SELECT ISNULL(SUM(CASE WHEN PZ.DruhPohybuZbo IN (0, 3) THEN VCP.Mnozstvi ELSE -VCP.Mnozstvi END), 0) FROM  TabVyrCP VCP WITH(NOLOCK)    INNER JOIN TabPohybyZbozi PZ WITH(NOLOCK) ON PZ.ID=VCP.IDPolozkaDokladu AND PZ.DruhPohybuZbo<5 AND PZ.SkutecneDatReal IS NULL   WHERE  VCP.IDVyrCis=TabVyrCS.ID)     +  (SELECT ISNULL(SUM(Mnozstvi), 0) FROM Gatema_SDScanData P WITH(NOLOCK) INNER JOIN Gatema_SDDoklady H WITH(NOLOCK) ON H.ID=P.IDDokladSD WHERE H.TypDokladu=533 AND H.DatGenerovani IS NULL AND P.VyrCislo=TabVyrCS.Nazev1)  )
