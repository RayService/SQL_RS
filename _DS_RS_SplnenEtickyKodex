CAST(  CASE WHEN EXISTS(  SELECT tu.ID  FROM TabUkoly tu WITH(NOLOCK)  LEFT OUTER JOIN TabKontaktJednani tkj WITH(NOLOCK) ON tu.IDKontaktJed=tkj.ID AND tu.Vzor=0  LEFT OUTER JOIN TabCisOrg tco WITH(NOLOCK) ON tco.CisloOrg=tkj.CisloOrg  WHERE((tu.VzorQMS=0)AND(NOT EXISTS(SELECT*FROM TabKategKontJed K WITH(NOLOCK) JOIN TabKontaktJednani KJ WITH(NOLOCK) ON KJ.Kategorie=K.Cislo WHERE K.QMSAgenda IS NOT NULL AND tu.IDKontaktJed=KJ.ID))  AND(tco.ID=TabCisOrg.ID)AND(tu.Stav=2)AND(tkj.Kategorie=N'072'))  ) THEN 1 ELSE 0 END AS BIT  )
