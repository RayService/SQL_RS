(  SELECT SUM(TabDokladyZbozi.SumaKcBezDPH-TabDokladyZbozi.EcSNCelkemDokl)  FROM TabDokladyZbozi WITH(NOLOCK)   JOIN TabObdobi WITH(NOLOCK)  ON TabObdobi.Id = TabDokladyZbozi.Obdobi  WHERE TabDokladyZbozi.DruhPohybuZbo IN (13,14) AND TabDokladyZbozi.StredNaklad LIKE N'200%' AND TabObdobi.Nazev=hvw_8E307B5CB0EC4197BF962CC0EE516670.Obdobi  AND TabDokladyZbozi.CisloOrg NOT IN (2283,6365) AND (TabDokladyZbozi.RadaDokladu not like N'29%' AND TabDokladyZbozi.RadaDokladu not like N'26%' AND TabDokladyZbozi.RadaDokladu not like N'27%' AND TabDokladyZbozi.RadaDokladu not like N'23%') AND TabDokladyZbozi.DatPorizeni_M = 3  )
