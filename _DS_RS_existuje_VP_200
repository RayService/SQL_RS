(CAST (CASE WHEN EXISTS(SELECT tpl.ID  FROM TabPlan tpl WITH(NOLOCK)  LEFT OUTER JOIN TabKmenZbozi tkz WITH(NOLOCK) ON tkz.ID=tpl.IDTabKmen  LEFT OUTER JOIN TabPrikaz tp WITH(NOLOCK) ON tp.IDPlan = tpl.ID AND tp.IDTabKmen=tpl.IDTabKmen  WHERE tp.Rada = '200' AND tp.StavPrikazu >=50 AND tkz.ID = (SELECT tp.IDTabKmen FROM TabPlan tp WHERE tp.ID=TabPlan.ID)) THEN 1  ELSE 0 END AS BIT))
