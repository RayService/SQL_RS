(SELECT ((TabDokladyZbozi.SumaKcbezDPH -((SUM(PZKR.mat)*1.15)+SUM(PZKR.koop)+(SUM(PZKR.mzda)*6.370)))/(TabDokladyZbozi.SumaKcbezDPH))*100 FROM TabPohybyZbKalkRoz PZKR WITH(NOLOCK)  INNER JOIN TabPohybyZbozi PZ WITH(NOLOCK) ON PZKR.IDPohyb = PZ.IDOldPolozka  WHERE PZ.IDDoklad = TabDokladyZbozi.ID AND TabDokladyZbozi.DruhPohybuZbo=13)
