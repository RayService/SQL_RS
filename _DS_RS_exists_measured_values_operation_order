(  CAST (CASE WHEN EXISTS    (SELECT tpp.ID      FROM TabPrPostup tpp WITH(NOLOCK)      JOIN B2A_MeasuredValueDefinition_ProductionOrderOperation mvpop WITH(NOLOCK) ON (mvpop.IDProductionOrder = tpp.IDPrikaz AND mvpop.DocNum = tpp.Doklad)      JOIN B2A_MeasuredValueDefinition mvd WITH(NOLOCK) ON mvd.ID = mvpop.IDDefinition   JOIN TabPrikaz tp WITH(NOLOCK) ON tp.ID = tpp.IDPrikaz      WHERE (tp.ID = TabPrikaz.ID AND tpp.IDOdchylkyDo IS NULL AND tpp.prednastaveno=1)) THEN 1 ELSE 0 END AS BIT)  )
