  (SELECT CASE WHEN   (SELECT  tss.Mnozstvi-ISNULL(tpkz.BlokovanoProVyrobu,0)-ISNULL(tkze._EXT_RS_MnozstviPlanovanaVyrobaLPP,0)  -ISNULL(tss.Rezervace,0))>0  THEN   (SELECT  tss.Mnozstvi-ISNULL(tpkz.BlokovanoProVyrobu,0)-ISNULL(tkze._EXT_RS_MnozstviPlanovanaVyrobaLPP,0)  -ISNULL(tss.Rezervace,0)  )*tss.Prumer  ELSE 0 END  FROM TabStavSkladu tss WITH(NOLOCK)  LEFT OUTER JOIN TabParametryKmeneZbozi tpkz WITH(NOLOCK) ON tss.IDKmenZbozi=tpkz.IDKmenZbozi  LEFT OUTER JOIN TabKmenZbozi tkz WITH(NOLOCK) ON tkz.ID=tss.IDKmenZbozi  LEFT OUTER JOIN TabKmenZbozi_EXT tkze WITH(NOLOCK) ON tkze.ID=tkz.ID  WHERE  (tss.IDSklad='100'/*:__SKLAD*/)AND(tss.ID=TabStavSkladu.ID)  )
