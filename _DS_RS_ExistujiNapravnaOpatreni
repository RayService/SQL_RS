(  CAST((CASE WHEN EXISTS (  SELECT  tkj.ID  FROM TabKontaktJednani tkj WITH(NOLOCK)  LEFT OUTER JOIN TabKategKontJed katkonj WITH(NOLOCK) ON tkj.Kategorie=katkonj.Cislo  WHERE  ((EXISTS(SELECT * FROM TabKategKontJed KTG WITH(NOLOCK) WHERE tkj.Kategorie=KTG.Cislo AND KTG.QMSAgenda IS NOT NULL))  AND(tkj.SouvisejiciKJ=TabKontaktJednani.ID)  AND(katkonj.QMSAgenda=4))  )  THEN 1 ELSE 0 END) AS BIT)  )
