(  SELECT  KK.Popis  FROM TabDokumenty td WITH(NOLOCK)  LEFT OUTER JOIN TabKontaktJednani tkj WITH(NOLOCK) ON EXISTS(SELECT*FROM TabDokumVazba V WITH(NOLOCK) JOIN TabKontaktJednani KJ WITH(NOLOCK) ON KJ.ID=V.IDTab AND KJ.ID=tkj.ID JOIN TabKategKontJed KK WITH(NOLOCK) ON KK.Cislo=KJ.Kategorie WHERE V.IDDok=td.ID AND V.IdentVazby=1 AND KK.QMSAgenda=8)  LEFT OUTER JOIN TabKategKontJed KK WITH(NOLOCK) ON tkj.Kategorie=KK.Cislo  WHERE td.ID = TabDokumenty.ID  )  
