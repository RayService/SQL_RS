CAST(CASE WHEN EXISTS(SELECT tp.ID  FROM TabPrikaz tp WITH(NOLOCK)  LEFT OUTER JOIN Gatema_TermETH gt WITH(NOLOCK) ON gt.IDPrikaz=tp.ID  LEFT OUTER JOIN TabPrPostup tpp WITH(NOLOCK) ON tpp.IDPrikaz=tp.ID  WHERE tp.ID=TabPrikaz.ID AND gt.IDKolegy IS NULL AND gt.Akce=10 AND tpp.nazev LIKE 'Oprava neshody%') THEN 1 ELSE 0 END AS BIT  )
