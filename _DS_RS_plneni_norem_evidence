(  SELECT  CASE WHEN tpmz.kusy_odv=0  THEN 0  ELSE (((tpp.Kusy_pozadovane * tpp.TAC_H / (tpp.DavkaTPV_VO)))+tpp.TBC_H+tpp.TEC_H)/tpp.Kusy_pozadovane*tpmz.kusy_odv/ISNULL(NULLIF(tpmz.Sk_cas_Obsluhy_H,0),1)*100 END  FROM TabPrikazMzdyAZmetky tpmz WITH(NOLOCK)  LEFT OUTER JOIN TabPrPostup tpp WITH(NOLOCK) ON tpp.IDPrikaz=tpmz.IDPrikaz AND tpmz.DokladPrPostup=tpp.Doklad AND tpmz.AltPrPostup=tpp.Alt AND tpp.IDOdchylkyDo IS NULL  WHERE tpmz.ID=TabPrikazMzdyAZmetky.ID  )
