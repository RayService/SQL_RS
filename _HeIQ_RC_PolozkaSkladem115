CAST (CASE WHEN EXISTS  (SELECT tkz.ID  FROM TabPohybyZbozi tpz WITH(NOLOCK)  LEFT OUTER JOIN TabStavSkladu tskl WITH(NOLOCK) ON tskl.ID=tpz.IDZboSklad  LEFT OUTER JOIN TabKmenZbozi tkz WITH(NOLOCK) ON tkz.ID=tskl.IDKmenZbozi  LEFT OUTER JOIN TabStavSkladu tss115 ON tss115.IDKmenZbozi=tkz.ID AND tss115.IDSklad=N'10000115'  WHERE tpz.ID=TabPohybyZbozi.ID AND tss115.Mnozstvi>0  ) THEN 2971904 ELSE NULL END AS INT)        
