CAST ((  CASE WHEN EXISTS (SELECT tpz2.ID  FROM TabPohybyZbozi tpz WITH(NOLOCK)  LEFT OUTER JOIN TabPohybyZbozi tpz2 WITH(NOLOCK) ON tpz2.IDOldPolozka = tpz.ID  LEFT OUTER JOIN TabDokladyZbozi tdz2 WITH(NOLOCK) ON tdz2.ID = tpz2.IDDoklad  LEFT OUTER JOIN TabDokladyZbozi tdz WITH(NOLOCK) ON tdz.ID = tpz.IDDoklad  JOIN TabZakazka tz WITH(NOLOCK) ON tz.CisloZakazky = tdz.CisloZakazky   WHERE tpz.ID = TabPohybyZbozi.ID AND ((tdz2.DruhPohybuZbo = 9 AND tdz2.RadaDokladu IN ('400','410','411','472','420','430','474')) OR (tz.Stav IN ('27','28'))))  THEN 1 ELSE 0 END) AS BIT)
