(  SELECT tj.Popis  FROM hvw_FB1D5911512844FE88E5160E6ACFBD40 matice WITH(NOLOCK)  LEFT OUTER JOIN TabCisZam tcz WITH(NOLOCK) ON tcz.ID=matice.IDEscalRS  LEFT OUTER JOIN TabCisZam_EXT tcze WITH(NOLOCK) ON tcze.ID=tcz.ID  LEFT OUTER JOIN TabJazyky tj ON tj.ID=tcze._EXT_RS_ComLanguage  WHERE matice.ID=hvw_FB1D5911512844FE88E5160E6ACFBD40.ID  )
