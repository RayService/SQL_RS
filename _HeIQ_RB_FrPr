CAST(CASE     WHEN  (  (SELECT ISNULL(SUM((tpp.Kusy_zad * tpp.TAC_S / (3600.0 * tpp.DavkaTPV_VO))+tpp.TBC_H),0)  FROM TabPrPostup tpp WITH(NOLOCK)  LEFT OUTER JOIN TabPrikaz tp WITH(NOLOCK) ON tpp.IDPrikaz=tp.ID  LEFT OUTER JOIN TabCOdchylek tco WITH(NOLOCK) ON tpp.IDOdchylkyOd=tco.ID  WHERE  ((tp.StavPrikazu IN (20,30) AND tpp.splneno=0 AND tpp.IDOdchylkyDo IS NULL)  AND((tpp.pracoviste=TabCPraco.ID))))  /  (convert(Numeric(19,6),(TabCPraco.smennost * 0.01 * TabCPraco.plneni * (SELECT CASE WHEN COUNT(*)=0 THEN 1 ELSE COUNT(*) END FROM TabCisStroju CS WITH(NOLOCK) WHERE CS.IDPrac=TabCPraco.ID) * TabCPraco.vice_lidi)))  )  < 8 THEN 4227327    WHEN  (  (SELECT ISNULL(SUM((tpp.Kusy_zad * tpp.TAC_S / (3600.0 * tpp.DavkaTPV_VO))+tpp.TBC_H),0)  FROM TabPrPostup tpp WITH(NOLOCK)  LEFT OUTER JOIN TabPrikaz tp WITH(NOLOCK) ON tpp.IDPrikaz=tp.ID  LEFT OUTER JOIN TabCOdchylek tco WITH(NOLOCK) ON tpp.IDOdchylkyOd=tco.ID  WHERE  ((tp.StavPrikazu IN (20,30) AND tpp.splneno=0 AND tpp.IDOdchylkyDo IS NULL)  AND((tpp.pracoviste=TabCPraco.ID))))  /  (convert(Numeric(19,6),(TabCPraco.smennost * 0.01 * TabCPraco.plneni * (SELECT CASE WHEN COUNT(*)=0 THEN 1 ELSE COUNT(*) END FROM TabCisStroju CS WITH(NOLOCK) WHERE CS.IDPrac=TabCPraco.ID) * TabCPraco.vice_lidi)))  )  < 11 THEN 65280    WHEN  (  (SELECT ISNULL(SUM((tpp.Kusy_zad * tpp.TAC_S / (3600.0 * tpp.DavkaTPV_VO))+tpp.TBC_H),0)  FROM TabPrPostup tpp WITH(NOLOCK)  LEFT OUTER JOIN TabPrikaz tp WITH(NOLOCK) ON tpp.IDPrikaz=tp.ID  LEFT OUTER JOIN TabCOdchylek tco WITH(NOLOCK) ON tpp.IDOdchylkyOd=tco.ID  WHERE  ((tp.StavPrikazu IN (20,30) AND tpp.splneno=0 AND tpp.IDOdchylkyDo IS NULL)  AND((tpp.pracoviste=TabCPraco.ID))))  /  (convert(Numeric(19,6),(TabCPraco.smennost * 0.01 * TabCPraco.plneni * (SELECT CASE WHEN COUNT(*)=0 THEN 1 ELSE COUNT(*) END FROM TabCisStroju CS WITH(NOLOCK) WHERE CS.IDPrac=TabCPraco.ID) * TabCPraco.vice_lidi)))  )  > 11 THEN 255      ELSE NULL    END AS INT)  
