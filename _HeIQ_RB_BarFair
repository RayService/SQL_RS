CAST  (CASE   WHEN   (SELECT _fair FROM TabPrikaz_EXT WITH(NOLOCK) WHERE ID=TabPrikaz.ID) = 1 THEN 9568250   WHEN   (SELECT _UvolSer FROM TabPrikaz_EXT WITH(NOLOCK) WHERE ID=TabPrikaz.ID) = 1 THEN 57343  WHEN   (SELECT tple._FAIR FROM TabPrikaz tp WITH(NOLOCK) LEFT OUTER JOIN TabPlan tpl WITH(NOLOCK) ON tpl.ID=tp.IDPlan LEFT OUTER JOIN TabPlan_EXT tple WITH(NOLOCK) ON tple.ID=tpl.ID WHERE tp.ID=TabPrikaz.ID) = 1 THEN 9568250  WHEN   (SELECT tple._FAIRint FROM TabPrikaz tp WITH(NOLOCK) LEFT OUTER JOIN TabPlan tpl WITH(NOLOCK) ON tpl.ID=tp.IDPlan LEFT OUTER JOIN TabPlan_EXT tple WITH(NOLOCK) ON tple.ID=tpl.ID WHERE tp.ID=TabPrikaz.ID) = 1 THEN 57343  --přidána původní podmínka  WHEN   (SELECT _fair FROM TabPrikaz_EXT WITH(NOLOCK) WHERE ID=TabPrikaz.ID) = 1 THEN 9568250   WHEN   (SELECT _UvolSer FROM TabPrikaz_EXT WITH(NOLOCK) WHERE ID=TabPrikaz.ID) = 1 THEN 57343  WHEN   (SELECT tple._FAIR FROM TabPrikaz tp WITH(NOLOCK) LEFT OUTER JOIN TabPlan tpl WITH(NOLOCK) ON tpl.ID=tp.IDPlan LEFT OUTER JOIN TabPlan_EXT tple WITH(NOLOCK) ON tple.ID=tpl.ID WHERE tp.ID=TabPrikaz.ID) = 1 THEN 9568250  WHEN   (SELECT tple._FAIRint FROM TabPrikaz tp WITH(NOLOCK) LEFT OUTER JOIN TabPlan tpl WITH(NOLOCK) ON tpl.ID=tp.IDPlan LEFT OUTER JOIN TabPlan_EXT tple WITH(NOLOCK) ON tple.ID=tpl.ID WHERE tp.ID=TabPrikaz.ID) = 1 THEN 57343     END AS INT)  
