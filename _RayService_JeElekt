CAST(CASE WHEN EXISTS (SELECT *   FROM TabPrKVazby PK WITH(NOLOCK)  INNER JOIN TabKmenZbozi KZ  WITH(NOLOCK)ON PK.nizsi=KZ.ID  INNER JOIN TabKmenZbozi_EXT KZE WITH(NOLOCK) ON KZE.ID=KZ.ID  WHERE KZE._elektrostatika=1  AND PK.IDOdchylkyDo IS NULL  AND PK.IDPrikaz = TabPrikaz.ID  AND PK.prednastaveno=1   AND PK.Splneno=0    )THEN 1 ELSE 0 END AS BIT) 
