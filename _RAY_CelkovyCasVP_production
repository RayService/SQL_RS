  (  SELECT SUM((tpp.Kusy_zad*tpp.tac_H)+tpp.TBC_H+tpp.TEC_H)  FROM TabPrPostup tpp  LEFT JOIN TabCPraco WITH(NOLOCK) ON TabCPraco.ID=tpp.Pracoviste  LEFT OUTER JOIN TabCPraco_EXT WITH(NOLOCK) ON TabCPraco_EXT.ID=TabCPraco.ID  WHERE tpp.IDPrikaz=TabPrikaz.ID AND (TabCPraco.IDTabStrom=N'200' OR (TabCPraco.IDTabStrom LIKE N'2002%'AND (ISNULL(TabCPraco_EXT._EXT_RS_wokplaceQA,0)=0 AND ISNULL(TabCPraco_EXT._EXT_RS_workplaceWH,0)=0))) AND tpp.IDOdchylkyDo IS NULL AND tpp.prednastaveno=1  )  
