(CAST (CASE WHEN EXISTS (SELECT *    FROM TabPrPostup WITH(NOLOCK)    LEFT OUTER JOIN TabCPraco WITH(NOLOCK) ON TabCPraco.ID=TabPrPostup.pracoviste    WHERE TabPrPostup.IDPrikaz = TabPrikaz.ID    AND (TabPrPostup.TypOznaceni = 'VF5' OR TabPrPostup.TypOznaceni = 'VF7')    AND (TabPrPostup.IDOdchylkyDo IS NULL)    AND (TabPrPostup.prednastaveno=1)    AND (TabCPraco.IDTabStrom=200)  )  THEN 1 ELSE 0 END AS BIT)  )
