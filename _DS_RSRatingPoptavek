(  SELECT ISNULL(OP.Vaha,0)+ISNULL(OV.Vaha,0)+ISNULL(NK.Vaha,0)+ISNULL(R.Vaha,0)+ISNULL(SP.Vaha,0)+ISNULL(ASP.Vaha,0)+ISNULL(tdce._EXT_RS_vaha_parametru,0)  FROM TabZakazka_EXT tze WITH(NOLOCK)  LEFT OUTER JOIN TabZakazka tz WITH(NOLOCK) ON tz.ID=tze.ID  LEFT OUTER JOIN Tabx_RS_RatingZakazek OP WITH(NOLOCK) ON OP.Parametr=tze._EXT_RS_opakovanost_poptavky AND OP.Rating='Opakovanost poptávky'  LEFT OUTER JOIN Tabx_RS_RatingZakazek OV WITH(NOLOCK) ON OV.Parametr=tze._EXT_RS_objem_vyroby AND OV.Rating='Objem výroby'  LEFT OUTER JOIN Tabx_RS_RatingZakazek NK WITH(NOLOCK) ON NK.Parametr=tze._EXT_RS_narocnost_kalkulace AND NK.Rating='Náročnost kalkulace'  LEFT OUTER JOIN Tabx_RS_RatingZakazek R WITH(NOLOCK) ON R.Parametr=tze._EXT_RS_rentabilita AND R.Rating='Rentabilita (segment)'  LEFT OUTER JOIN Tabx_RS_RatingZakazek SP WITH(NOLOCK) ON SP.Parametr=tze._EXT_RS_seriovy_potencial AND SP.Rating='Sériový potenciál'  LEFT OUTER JOIN Tabx_RS_RatingZakazek ASP WITH(NOLOCK) ON ASP.Parametr=tze._EXT_RS_aspekt AND ASP.Rating='Politický aspekt'  LEFT OUTER JOIN TabCisOrg tco WITH(NOLOCK) ON tco.CisloOrg=tz.Prijemce  JOIN TabDruhCinnosti tdc WITH(NOLOCK) ON tdc.ID=tco.DruhCinnosti  LEFT OUTER JOIN TabDruhCinnosti_EXT tdce WITH(NOLOCK) ON tdce.ID=tdc.ID  WHERE tze.ID=TabZakazka.ID  )
