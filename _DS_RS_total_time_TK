((  SELECT ((prp.Kusy_zad * prp.TAC_H / (prp.DavkaTPV_VO)))+prp.TBC_H AS Total_time_TK  FROM TabPrPostup prp WITH(NOLOCK)  LEFT OUTER JOIN TabCPraco tcpo WITH(NOLOCK) ON prp.Pracoviste=tcpo.ID  LEFT OUTER JOIN TabCPraco_EXT  WITH(NOLOCK) ON TabCPraco_EXT.ID=tcpo.ID  WHERE (tcpo.IDTabStrom LIKE N'2000026%' OR (tcpo.IDTabStrom LIKE N'2002%'AND ISNULL(TabCPraco_EXT._EXT_RS_wokplaceQA,0)=1)) AND prp.IDOdchylkyDo IS NULL AND prp.ID=hvw_B1E35595F5B2424AAC7695CCCB8F6EE2.ID  )  UNION  (  SELECT ((plprp.Kusy_zad * plprp.TAC_H / (plprp.DavkaTPV_VO)))+plprp.TBC_H AS Total_time_TK  FROM TabPlanPrPostup plprp WITH(NOLOCK)  LEFT OUTER JOIN TabCPraco tcpo WITH(NOLOCK) ON plprp.Pracoviste=tcpo.ID  LEFT OUTER JOIN TabCPraco_EXT  WITH(NOLOCK) ON TabCPraco_EXT.ID=tcpo.ID  WHERE (tcpo.IDTabStrom LIKE N'2000026%' OR (tcpo.IDTabStrom LIKE N'2002%'AND ISNULL(TabCPraco_EXT._EXT_RS_wokplaceQA,0)=1)) AND plprp.ID=hvw_B1E35595F5B2424AAC7695CCCB8F6EE2.ID  ))
