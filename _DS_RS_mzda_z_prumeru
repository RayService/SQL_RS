(  SELECT tzm.DovPrumer*tpmz.Sk_cas_Obsluhy_H  FROM TabPrikazMzdyAZmetky tpmz WITH(NOLOCK)  LEFT OUTER JOIN TabCisZam tcz WITH(NOLOCK) ON tcz.ID=tpmz.Zamestnanec  JOIN TabZamMzd tzm WITH(NOLOCK) ON tzm.ZamestnanecId=tcz.ID  LEFT OUTER JOIN TabMzdObd obd WITH(NOLOCK) ON (obd.ID=tzm.IdObdobi AND obd.Rok=tpmz.datum_Y AND obd.Mesic=tpmz.datum_M)  WHERE tpmz.ID = TabPrikazMzdyAZmetky.ID/*2076960*/ AND obd.ID IS NOT NULL  )
