((TabPohybyZbozi.Mnozstvi- (SELECT ISNULL(SUM(P.Mnozstvi),0) FROM TabPohybyZbozi P WITH(NOLOCK)  WHERE P.IDOldPolozka = TabPohybyZbozi.ID)) *     ( SELECT(  SELECT sum(TAC_J_H)       FROM (SELECT  nizsi FROM TabKVazby where  TabKVazby.ZmenaDo is null AND vyssi=K.ID UNION ALL SELECT K.ID) s       JOIN TabPostup p WITH(NOLOCK) ON s.nizsi = p.dilec       JOIN  TabKmenZbozi TKZ WITH(NOLOCK) ON TKZ.ID= s.nizsi       WHERE p.Typ = 1  AND  p.ZmenaDo is null   )  FROM TabStavSkladu S WITH(NOLOCK) INNER JOIN TabKmenZbozi K WITH(NOLOCK) ON S.IDKmenZbozi=K.ID   INNER JOIN TabPohybyZbozi P WITH(NOLOCK) ON S.ID=P.IDZboSklad   WHERE P.DruhPohybuZbo IN (9,11) AND P.ID=TabPohybyZbozi.ID  ))  
