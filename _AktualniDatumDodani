CASE   WHEN (SELECT PZE._dat_dod FROM TabPohybyZbozi_EXT PZE WITH(NOLOCK) WHERE PZE.ID=TabPohybyZbozi.ID) IS NOT NULL THEN (SELECT PZE._dat_dod FROM TabPohybyZbozi_EXT PZE WITH(NOLOCK) WHERE PZE.ID=TabPohybyZbozi.ID)   WHEN (SELECT PZ.PotvrzDatDod_X  FROM TabPohybyZbozi PZ WITH(NOLOCK) WHERE PZ.ID=TabPohybyZbozi.ID) IS NOT NULL THEN (SELECT PZ.PotvrzDatDod_X  FROM TabPohybyZbozi PZ WITH(NOLOCK) WHERE PZ.ID=TabPohybyZbozi.ID)  WHEN (SELECT PZ.PozadDatDod_X FROM TabPohybyZbozi PZ WITH(NOLOCK) WHERE PZ.ID=TabPohybyZbozi.ID) IS NOT NULL THEN (SELECT PZ.PozadDatDod_X FROM TabPohybyZbozi PZ WITH(NOLOCK) WHERE PZ.ID=TabPohybyZbozi.ID)   ELSE  (SELECT DZ.DatPorizeni_X FROM  tabDokladyZbozi DZ WITH(NOLOCK) LEFT JOIN TabPohybyZbozi PZ WITH(NOLOCK) ON PZ.IdDoklad=DZ.ID  WHERE PZ.ID=TabPohybyZbozi.ID)  END 
