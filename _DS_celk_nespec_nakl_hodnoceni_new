(  CASE  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 95 AND 115 THEN 1  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 90 AND 94.999 THEN 2  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 115.0001 AND 120 THEN 2  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 85 AND 89.999 THEN 3  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 BETWEEN 120.001 AND 125 THEN 3  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 < 85 THEN 4  WHEN ((#TabPorPS.Celkem_Pl+#TabPorPS.NespecNakl)/#TabPorPS.Celkem)*100 > 125 THEN 4  WHEN (SELECT Celkem FROM #TabPorPS PPS WHERE PPS.ID= #TabPorPS.ID) = 0 THEN NULL  ELSE  NULL  END  )
