(((SELECT (SUM(PP.Kusy_zad * PP.TAC_S / (3600.0 * PP.DavkaTPV_VO))) FROM TabPrPostup PP WITH(NOLOCK) LEFT JOIN TabCPraco WITH(NOLOCK) ON TabCPraco.ID=PP.Pracoviste  LEFT JOIN TabPrikaz P WITH(NOLOCK) ON PP.IDPrikaz=P.ID   WHERE (TabCPraco.IDTabStrom=N'200' OR TabCPraco.IDTabStrom LIKE N'2002%') AND P.ID=TabPrikaz.ID)))  /  ((DATEDIFF(day, TabPrikaz.Plan_zadani_X,TabPrikaz.Plan_ukonceni_X ))+0.0000001)
