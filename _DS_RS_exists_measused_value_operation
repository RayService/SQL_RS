(  CAST (CASE WHEN EXISTS    (SELECT p.ID      FROM TabPostup p WITH(NOLOCK)      JOIN B2A_MeasuredValueDefinition_PdmOperation mvop WITH(NOLOCK) ON (mvop.IDPdm = p.dilec AND mvop.Operation = p.Operace)      JOIN B2A_MeasuredValueDefinition mvd WITH(NOLOCK) ON mvd.ID = mvop.IDDefinition   JOIN TabKmenZbozi tkz WITH(NOLOCK) ON tkz.ID = p.dilec      WHERE tkz.ID = TabKmenZbozi.ID AND p.ZmenaDo IS NULL) THEN 1 ELSE 0 END AS BIT)  )
