(  SELECT            cast(( SELECT nazev + ','               FROM               (SELECT    distinct    cast(tp.nazev as nvarchar(50)) as nazev, TabKmenZbozi.ID  FROM            TabPostup AS tp WITH(NOLOCK)                         INNER JOIN TabKmenZbozi AS tkz WITH(NOLOCK) ON tp.dilec = tkz.ID                                                               WHERE (tkz.ID = TabKmenZbozi.ID AND tp.ZmenaDo IS NULL)         ) t2              WHERE t2.ID = t1.ID              ORDER BY nazev                FOR XML PATH('') )  as nvarchar(255))AS nazev          FROM      (SELECT    distinct    cast(tp.nazev as nvarchar(50)) as nazev, TabKmenZbozi.ID  FROM            TabPostup AS tp WITH(NOLOCK)                         INNER JOIN TabKmenZbozi AS tkz WITH(NOLOCK) ON tp.dilec = tkz.ID                                                               WHERE (tkz.ID = TabKmenZbozi.ID AND tp.ZmenaDo IS NULL)      ) t1          GROUP BY ID   )
