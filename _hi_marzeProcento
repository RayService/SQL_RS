(     SELECT (CASE WHEN (ISNULL(SNV.KcCelkemDruh,0)+PZ.CCEvidSNdruh) <> 0 THEN      ((PZ.CCbezDaniKcPoSdruhova - (ISNULL(SNV.KCCelkemDruh,0) + PZ.CCEvidSNdruh))/                           (ISNULL(SNV.KcCelkemDruh,0)+PZ.CCEvidSNdruh))*100      ELSE 0 END)     FROM TabPohybyZbozi PZ WITH(NOLOCK)     JOIN TabPohybySNView SNV WITH(NOLOCK) ON PZ.ID = SNV.ID     WHERE TabPohybyZbozi.ID = PZ.ID  )      
