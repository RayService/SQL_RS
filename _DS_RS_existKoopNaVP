CAST (CASE WHEN EXISTS(SELECT tpp.ID  FROM TabPrPostup tpp WITH (NOLOCK)  LEFT OUTER JOIN TabPrikaz tp WITH (NOLOCK) ON tpp.IDPrikaz=tp.ID AND tpp.IDOdchylkyDo IS NULL AND tpp.typ=2  LEFT OUTER JOIN TabPohybyZbozi tpz WITH (NOLOCK) ON tpz.TypVyrobnihoDokladu IN (0,1,2,3,5,6,9) AND tpz.IDPrikaz=tp.ID  LEFT OUTER JOIN TabDokladyZbozi tdz WITH (NOLOCK) ON tdz.ID=tpz.IDDoklad  WHERE (tdz.ID=TabDokladyZbozi.ID)  ) THEN 1 ELSE 0 END AS BIT)
