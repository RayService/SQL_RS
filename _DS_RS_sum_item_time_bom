(  SELECT  SUM(tkve._EXT_RS_item_time)  FROM TabKusovnik tk WITH(NOLOCK)  LEFT OUTER JOIN TabKmenZbozi tkz WITH(NOLOCK) ON tk.IDKmenZbozi=tkz.ID  LEFT OUTER JOIN TabKVazby tkv WITH(NOLOCK) ON tkv.vyssi = tk.IDFinal AND tkv.nizsi = tk.IDKmenZbozi  LEFT OUTER JOIN TabKvazby_EXT tkve WITH(NOLOCK) ON tkve.ID = tkv.ID  WHERE  tk.autor = SUSER_SNAME() AND tkv.vyssi = tk.IDFinal AND tkv.nizsi = tk.IDKmenZbozi AND tkv.ZmenaDo IS NULL AND tk.ID = TabKusovnik.ID  )
